<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.rh.ssh.model">
	<class name="TbProDetail" table="tb_pro_detail">
		<id name="proDetailId" type="java.lang.Integer">
			<column name="pro_detail_id" />
			<generator class="native"></generator>
		</id>
		<property name="proDetailFromTask" type="java.lang.String">
			<column name="pro_detail_from_task"></column>
		</property>
		<property name="proDetailCon" type="java.lang.String">
			<column name="pro_detail_con"></column>
		</property>
	</class>
</hibernate-mapping>