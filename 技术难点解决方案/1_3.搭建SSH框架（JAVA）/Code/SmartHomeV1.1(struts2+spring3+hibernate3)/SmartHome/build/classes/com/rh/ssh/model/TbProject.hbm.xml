<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.rh.ssh.model">
	<class name="TbProject" table="tb_project">
		<id name="proId" type="java.lang.Integer">
			<column name="pro_id" />
			<generator class="native"></generator>
		</id>
		<property name="proName" type="java.lang.String">
			<column name="pro_name"></column>
		</property>
		<property name="proIntro" type="java.lang.String">
			<column name="pro_intro"></column>
		</property>
		<property name="userId" type="java.lang.Integer">
			<column name="user_id"></column>
		</property>
	</class>
</hibernate-mapping>